    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.17.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "postcss": "^8.4.14",
    "postcss-advanced-variables": "^3.0.1",
